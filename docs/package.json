{
    "name": "docs.comentario.app",
    "version": "1.0.0",
    "description": "docs.comentario.app website",
    "scripts": {
        "clean": "rm -rf public",
        "build": "hugo --gc --minify",
        "build:dev": "hugo -DF --gc --minify --baseURL=https://edge.docs.comentario.app/",
        "postbuild": "echo '/*\\n  Access-Control-Allow-Origin: *' > public/_headers",
        "postbuild:dev": "echo '/*\\n  Access-Control-Allow-Origin: *' > public/_headers",
        "prebuild": "npm run clean",
        "prebuild:dev": "npm run clean",
        "start": "hugo server -DF"
    },
    "repository": "https://gitlab.com/comentario/docs.comentario.app",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.4.2",
        "@popperjs/core": "^2.11.8",
        "bootstrap": "^5.3.1",
        "jquery": "^3.7.1"
    }
}
