{
    "name": "comentario-frontend",
    "version": "3.0.0",
    "private": true,
    "scripts": {
        "build": "ng build --configuration development",
        "build:prod": "ng build",
        "generate": "rm -rf generated-api/ && openapi-generator-cli generate -i ../resources/swagger/swagger.yml -g typescript-angular -o generated-api",
        "lint": "eslint",
        "start": "ng build --watch --configuration development",
        "test": "ng test",
        "test:ci": "ng test --browsers=ChromeHeadlessCI --watch=false --progress=false",
        "copy-assets": "cp ../build/frontend/en/*.ico ../build/frontend/en/*.png ../build/frontend/en/*.svg ../build/frontend/en/*.xml ../build/frontend/en/*.txt ../build/frontend/en/site.webmanifest ../build/frontend/",
        "cleanup-assets": "rm -f ../build/frontend/*/*.ico ../build/frontend/*/*.png ../build/frontend/*/*.svg ../build/frontend/*/*.xml ../build/frontend/*/*.txt ../build/frontend/*/site.webmanifest",
        "postbuild": "npm run copy-assets && npm run cleanup-assets",
        "postbuild:prod": "npm run copy-assets && npm run cleanup-assets"
    },
    "dependencies": {
        "@angular/animations": "^18.2.7",
        "@angular/common": "^18.2.7",
        "@angular/compiler": "^18.2.7",
        "@angular/core": "^18.2.7",
        "@angular/forms": "^18.2.7",
        "@angular/localize": "^18.2.7",
        "@angular/platform-browser": "^18.2.7",
        "@angular/platform-browser-dynamic": "^18.2.7",
        "@angular/router": "^18.2.7",
        "@fortawesome/angular-fontawesome": "^0.15.0",
        "@fortawesome/fontawesome-svg-core": "^6.6.0",
        "@fortawesome/free-brands-svg-icons": "^6.6.0",
        "@fortawesome/free-regular-svg-icons": "^6.6.0",
        "@fortawesome/free-solid-svg-icons": "^6.6.0",
        "@ng-bootstrap/ng-bootstrap": "^17.0.1",
        "@ngneat/until-destroy": "^10.0.0",
        "@popperjs/core": "^2.11.8",
        "bootstrap": "^5.3.3",
        "chart.js": "^4.4.4",
        "ng2-charts": "^5.0.4",
        "ngx-highlightjs": "^12.0.0",
        "rxjs": "^7.8.1",
        "tslib": "^2.7.0",
        "zone.js": "^0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.2.8",
        "@angular-devkit/core": "^18.2.8",
        "@angular-devkit/schematics": "^18.2.8",
        "@angular/cli": "^18.2.8",
        "@angular/compiler-cli": "^18.2.7",
        "@openapitools/openapi-generator-cli": "^2.14.0",
        "@types/jasmine": "^5.1.4",
        "angular-eslint": "^18.3.1",
        "jasmine-core": "^5.3.0",
        "karma": "^6.4.4",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage": "^2.2.1",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "ng-mocks": "^14.13.1"
    },
    "peerDependencies": {
        "@angular/cdk": "^18.2.8"
    }
}
